---
---
[
 {%- for pg in site.pages -%}
   {%- if pg.visible == true -%}
     {
       "isproject":{%- if pg.layout == 'project' -%}true{%- else -%}false{%- endif -%},
       "type":"{{pg.type}}",
       "status":"{{pg.status}}",
       "url":"{{site.url}}{{pg.url}}",
       "name": "{{pg.title}}",
       "lang":"{{pg.lang}}",
       "start":"{{pg.start}}",
       "end":"{{pg.end}}"
     }{% unless forloop.last %},{% endunless %}
   {%- endif -%}
 {%- endfor -%}
]
