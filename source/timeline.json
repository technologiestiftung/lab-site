---
---
[
  {% assign i=0 %}
 {%- for pg in site.pages -%}
   {%- if pg.visible == true -%}
   {% if i > 0 %},{% endif %}{
       "isproject":{%- if pg.layout == 'project' -%}true{%- else -%}false{%- endif -%},
       "type":"{{pg.type}}",
       "status":"{{pg.status}}",
       "url":"/{{pg.url}}",
       "name": "{{pg.title}}",
       "lang":"{{pg.lang}}",
       "start":"{{pg.start}}",
       "end":"{{pg.end}}"
     }
     {% assign i = i | plus:1  %}
   {%- endif -%}
 {%- endfor -%}
]
